{
  "ASCII Art": {
    "topic": "ASCII Art",
    "description": "Convert an Image to an ASCII Art form that is stored in a text file",
    "matching": "ascii, ascii art, art",
    "args": "img_path: str, output_file: str",
    "returns": "str",
    "example": {
      "Image in Same Directory": "pywhatkit.image_to_ascii_art('Example.png')",
      "Image in Different Directory": "pywhatkit.image_to_ascii_art('C:\\Example\\Example.png')",
      "Change Output Filename": "pywhatkit.image_to_ascii_art('Example.png', 'filename')"
    },
    "link": "https://github.com/Ankit404butfound/PyWhatKit/wiki/ASCII-Art"
  },
